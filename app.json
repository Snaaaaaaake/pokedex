{
  "expo": {
    "name": "Pokedex",
	"description": "Карманный справочник по покемонам!",
	"platforms": ["android"],
    "slug": "pokedex_expo",
    "version": "1.0.2",
    "orientation": "default",
    "icon": "./assets/pokeball.png",
    "splash": {
      "image": "./assets/pokeball.png",
      "resizeMode": "contain",
      "backgroundColor": "#BF3030"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
	"package": "com.methegreat.pokedex_expo_app",
	"versionCode": 1,
	"permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/pokeball.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/pokeball.png"
    }
  }
}
